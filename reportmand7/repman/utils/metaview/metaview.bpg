#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = metaview.exe metaview.ESN
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

metaview.exe: metaview.dpr
  $(DCC)

metaview.ESN: ESN\metaview.dpr
  $(DCC)


